{
    "version": 2,
    "builds": [
      { "src": "api/index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/get", "dest": "/index.js" },
        { "src": "/api/post", "dest": "/index.js" },
        { "src": "/api/remove/:id", "dest": "/index.js" },
        { "src": "/api/get/:id", "dest": "/index.js" },
        { "src": "/api/update/:id", "dest": "/index.js" }
    ]
  }
  